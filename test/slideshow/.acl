@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix card: <https://www.w3.org/People/Berners-Lee/card#>.
@prefix n0: <http://www.w3.org/ns/auth/acl#>.
@prefix Pho: <./>.

<#ControlReadWrite>
    a    n0:Authorization;
    n0:accessTo
       Pho:;
    n0:agent
       card:i;
    n0:defaultForNew
       Pho:;
    n0:mode
       n0:Control, n0:Read, n0:Write.
<#Read>
    a    n0:Authorization;
    n0:accessTo
       Pho:;
    n0:agentClass
       foaf:Agent;
    n0:defaultForNew
       Pho:;
    n0:mode
       n0:Read.
<#AppendRead>
    a    n0:Authorization;
    n0:accessTo
       Pho:;
    n0:defaultForNew
       Pho:;
    n0:mode
       n0:Append, n0:Read.
